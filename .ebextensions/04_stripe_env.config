# .ebextensions/04_stripe_env.config
# Configuration to set Stripe environment variables

option_settings:
  aws:elasticbeanstalk:application:environment:
    STRIPE_PUBLISHABLE_KEY: "YOUR_STRIPE_PUBLISHABLE_KEY"
    STRIPE_SECRET_KEY: "YOUR_STRIPE_SECRET_KEY"
    # STRIPE_WEBHOOK_SECRET is not provided, leaving it out for now.
